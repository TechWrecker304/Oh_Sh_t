{
  "manifest_version": 2,
  "name": "Oh Sh%t! Quick Access Menu",
  "version": "1.0",
  "description": "A Chrome extension to quickly access important websites.",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": ["tabs"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
